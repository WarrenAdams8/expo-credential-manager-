{
  "name": "expo-credential-manager",
  "version": "0.1.0",
  "description": "Expo module exposing Android Credential Manager (passkeys and passwords).",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest"
  },
  "keywords": [
    "expo",
    "android",
    "credential-manager",
    "passkey",
    "webauthn"
  ],
  "peerDependencies": {
    "expo": ">=50.0.0",
    "expo-modules-core": ">=1.11.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.ts"],
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"],
    "clearMocks": true
  }
}
